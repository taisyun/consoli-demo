    "webpack": "4.28.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-dev-server": "3.1.2",
    "webpack-hot-middleware": "2.24.3"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "connect-history-api-fallback": "^1.1.0",
    "express": "^4.13.3",
    "history": "^2.0.0",
