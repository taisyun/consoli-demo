{
  "name": "hrdemo-websocket",
  "version": "0.0.0",
  "description": "hrdemo-websocket",
  "main": "./src/index.js",
  "scripts": {
    "start1": "babel-node ./src/index.js",
    "start2": "babel-node ./src/index_server.js",
    "start": "npm run start1",
    "debug1": "babel-node --inspect --inspect-brk ./src/index.js",
    "debug2": "babel-node --inspect --inspect-brk ./src/index_server.js",
    "debug": "npm run start1",
    "test:server": "mocha --compilers js:babel/register --recursive './src/server/test/**/*.@(js|jsx)'",
    "test:client": "mocha --compilers js:babel-core/register --require ./src/app/test/test_helper.js --recursive './src/app/test/**/*.@(js|jsx)'",
    "test": "npm run test:server && npm run test:client",
    "test:server-watch": "npm run test:server -- --watch",
    "test:client-watch": "npm run test:client -- --watch",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "^6.0.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "copyfiles": "^0.2.1",
    "css-loader": "2.1.0",
    "debug": "^2.2.0",
    "eslint": "^1.7.3",
    "eslint-config-rackt": "^1.1.1",
    "eslint-config-reactjs": "^1.0.0",
    "eslint-plugin-objects": "^1.1.1",
    "eslint-plugin-react": "^3.6.3",
    "expose-loader": "0.7.5",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "jsdom": "^7.2.2",
    "json-loader": "0.5.7",
    "mocha": "^2.4.5",
    "morgan": "^1.6.1",
    "postcss": "7.0.7",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "4.6.3",
    "redux-devtools": "^3.1.1",
